(func atag "<a href='{0}' class='{1}'>{2}</a>")
(func page (atag {0} "pages" {1}))
(func li "\t<li>{0}</li>")
(func ul "<ul>\n{0}\n</ul>")
(li "hello")
(li (page "green" "colour"))
;(ul (map li (eval "cat test.slisp")))
(li (concat " " "this is a silly little thing and" (page "hi" "hello") "and" (page "blue" "green")))
